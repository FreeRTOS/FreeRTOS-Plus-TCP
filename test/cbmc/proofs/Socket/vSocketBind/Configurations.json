{
  "ENTRY": "vSocketBind",
  "ALLOW_TCP":"1",
  "CBMCFLAGS":
  [
    "--unwind 1"
  ],
  "OBJS":
  [
    "$(FREERTOS_PLUS_TCP)/test/FreeRTOS-Kernel/list.goto",
    "$(FREERTOS_PLUS_TCP)/FreeRTOS_Routing.goto",
    "$(FREERTOS_PLUS_TCP)/FreeRTOS_IP.goto",
    "$(FREERTOS_PLUS_TCP)/FreeRTOS_Sockets.goto",
    "$(ENTRY)_harness.goto"
  ],
  "INSTFLAGS":
  [
  ],
  "DEF":
  [
    {
      "ALLOW_ETHERNET_DRIVER_FILTERS_PACKETS": [
        "ipconfigALLOW_SOCKET_SEND_WITHOUT_BIND=1",
        "ipconfigUSE_TCP={ALLOW_TCP}",
        "ipconfigETHERNET_DRIVER_FILTERS_PACKETS=1"
      ]
    },
    {
      "ALLOW_SOCKET_SEND_WITHOUT_BIND": [
        "ipconfigALLOW_SOCKET_SEND_WITHOUT_BIND=1",
        "ipconfigUSE_TCP={ALLOW_TCP}"
      ]
    },
    {
      "DONT_ALLOW_SOCKET_SEND_WITHOUT_BIND": [
        "ipconfigALLOW_SOCKET_SEND_WITHOUT_BIND=0",
        "ipconfigUSE_TCP={ALLOW_TCP}"
      ]
    }
  ],
  "INC":
  [
    "$(FREERTOS_PLUS_TCP)/test/cbmc/include",
    "$(FREERTOS_PLUS_TCP)/test/cbmc/proofs/utility"
  ]
}

