{
  "ENTRY": "TCPAddRxdata",
  "CBMCFLAGS":
  [
    "--unwind 1"
  ],
  "INSTFLAGS":
  [
    "--generate-function-body 'vTCPStateChange|xSendEventToIPTask|uxStreamBufferAdd|uxStreamBufferFrontSpace'",
    "--generate-function-body-options nondet-return"
  ],
  "OBJS":
  [
    "$(ENTRY)_harness.goto",
    "$(FREERTOS_PLUS_TCP)/source/FreeRTOS_Sockets.goto"
  ],
  "OPT":
  [
    "--export-file-local-symbols"
  ],
  "INC":
  [
    "$(FREERTOS_PLUS_TCP)/test/cbmc/include",
    "$(FREERTOS_PLUS_TCP)/test/cbmc/proofs/utility"
  ]
}

